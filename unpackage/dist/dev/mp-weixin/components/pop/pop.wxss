/* uni.scss */
.closeIcon {
  position: absolute;
  top: 0;
  right: 0;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  background-color: #fff;
}
.imgBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
}
.imgBox .imgBox-item {
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
}
.imgBox .imgBox-item .imgBox-img {
  padding: 5rpx;
  width: 180rpx;
  height: 180rpx;
}
.imgchoose {
  height: 200rpx;
  width: 100%;
  background-color: #fff;
  left: 0;
}
.mask {
  position: absolute;
  width: 100vw;
  height: 100vh;
  left: 0;
  top: 0;
  z-index: 99999999999999999;
}

